<div class="container">
  <% if (sectionTitle) { %>
    <div class="vbox">
      <h6><%= sectionTitle %></h6>
    </div>
  <% } %>
  <div class="vbox">
    <p><%= content %></p>
  </div>
  <div class="vbox">
    <div class="hbox">
      <img src="<%= image1 %>">
      <p><%= content1 %></p>
    </div>
    <div class="hbox">
      <img src="<%= image2 %>">
      <p><%= content2 %></p>
    </div>
    <div class="hbox">
      <img src="<%= image3 %>">
      <p><%= content3 %></p>
    </div>
    <div class="hbox">
      <img src="<%= image4 %>">
      <p><%= content4 %></p>
    </div>
  </div>
</div>
